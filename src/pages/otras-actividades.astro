---
import Layout from '~/layouts/PageLayout.astro';

const metadata = { title: 'Otras actividades' };
---

<Layout metadata={metadata}>
  <section class="py-16 md:py-24">
    <div class="container mx-auto px-6 max-w-4xl">
      <h1 class="text-3xl md:text-4xl font-bold mb-4">Otras actividades</h1>
      <p class="text-lg text-muted">Talleres, encuentros y experiencias para tu bienestar integral.</p>
    </div>
  </section>
</Layout>

